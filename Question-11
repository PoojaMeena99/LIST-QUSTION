def fun():
    # a=int(input("enter no."))
    l=[]
    i=2
    while i<=10:
        l.append(i)
        print(l)
        i=i+2
fun()    


def fun():
    a=[]
    for i in range(1,11,3):
        a.append(i)
    print(a)
fun()
        
