i=1
b=[]
s=[]
while i<=30:
    if i<=5:
        b.append(i*i)
    if i>25 and i<=30:
        s.append(i*i)
    i+=1
print(b,s)
    


a=[23,56,12,34,78,59,90,12,45,67,]
i=0
b=[]
while i<len(a):
    c=[a[i],a[i+1]]
    b.append(c)
    i+=2
print(b)



i=int(input("starting"))
b=int(input("ending"))
c=[]
while i<=b:
    s=i*i,i*i+1
    # p=i*i
    c.append(s)
    i+=2
print(c)
