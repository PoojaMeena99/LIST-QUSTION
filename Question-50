i=0
max=0
d=[]
while i<len(a):
    n=a.count(a[i])
    if a[i]>max:
        max=a[i]
        b=n
    i=i+1
print(max,b)



"FIND MAX ELEMENT IN LIST "

def fun():
    a=[1,2,2,4,5,6,6]
    i=0
    max=0
    d=[]
    while i<len(a):
        n=a.count(a[i])
        if a[i]>max:
            max=a[i]
            b=n
        i=i+1
    print(max,b)
fun()    


