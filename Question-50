a=input("enter no.")
i=0
vobel=1
cons=1
while i<len(a):
    if a[i]==a[i]=="a" or a[i]=="e" or a[i]=="i" or a[i]=="o" or a[i]== "u":
        print((a[i]),"vobel",vobel)
        vobel=vobel+1
    else:
        print((a[i]),"cons",cons)
        cons=cons+1
    i=i+1


a=int(input("enter the no."))
b=int(input("enter the no"))
c=input("enter the operator ")
if  c=="+":
    print(a+b)
elif c=="-":
    print(a-b)
elif c=="*":
    print(a*b)
elif c=="/":
    print(a/b)
elif c=="**":
    print(a**b)
elif c=="%":
    print(a%b)
elif c=="//":
    print(a//b)       
else:
    print("error")       





def fun(a,b):
    i=0
    while i<len(a):
        if a[i]%b[i]==0:
            print("even")
        else:
            print("odd")  
        i=i+1
fun([1,2,3,4,5],[2,3,5,4,7])          




