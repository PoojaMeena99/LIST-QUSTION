def fun():
    weight=int(input("enter weight"))
    highte=int(input("enter the highter"))
    bmi=weight//highte
    if bmi<=18 :
        return("underweight")
    if bmi<=25:
        return("normal") 
    if bmi<=30:
        return("overweight")
    if bmi>30:
        return("obese")       
print (fun())
